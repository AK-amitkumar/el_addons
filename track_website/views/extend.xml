<?xml version="1.0"?>
<odoo>
    <template id="hello_extended"
              name="Extended Hello World"
              inherit_id="track_website.hello">
        <xpath expr="//h1" position="replace">
            <h1>
                Hello <t t-esc="name or 'Someone'"/>!
            </h1>
        </xpath>
    </template>
</odoo>
